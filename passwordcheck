#include<stdio.h>
#include<string.h>
int check_pass(char *p)
{
   //printf(" [+] inside the function  : %s\n",p);
   //checking the char of pass
   int count=0;
   int lowe=0;
   int a,b,c,d,f;
   int sum=0;
   if(strlen(p)<=5)
   {
      printf("[1]password is weak \n");
     
   }
   else if(strlen(p)>=5)
   {
      printf("[1]password is strong \n");
      
       a=1;
        //printf("%d \n",count);
   }
   //uppercase
   for(char i=65;i<=90;i++)
   {
      for(int j=0;j<=23;j++)
      {
        if(p[j]==i)
        {
           printf("[2]uppercase passed \n");
           
          
           b=1;
           break;
          
           
           
         }
         
         
         
         
      }
      
      
      
      
   }
   for(char i=97;i<=122;i++)
   {
      for(int j=0;j<=12;j++)
      {
        if(p[j]==i)
        {
           
           lowe++;
         
                   
   
        }
        
      }
      
      
   }
   if(lowe>=3)
        {
           printf("[3] 3 lowertest case test  passed\n");
           c=1;
        }
   else 
   {
      printf("[3]password should have 3 small letter  \n");
   }
   for(char i=33;i<=39;i++)
   {
      for(int j=0;j<=strlen(p);j++)
      {
         char k=64;
         if(p[j]==i)
         {
            printf("[4]symbol test case passed\n");
            
             d=1;
            
             
         }
         else if(p[j]==k)
         {
            printf("[4]symbol test case passed\n");
           d=1;

         }
        
          
           
        
         
      }
      
         break;
   }
   //numbering 
   for(char i=48;i<=57;i++)
   {
      for(int j=0;j<=13;j++)
      {
         if(p[j]==i)
         {
          printf("[5]number is satisfied \n")  ;
           
           f=1;
           break;
         
          
         }
         
         
          
      }
     
   }
   sum=a+b+c+d+f;
   printf("%d \n",sum);
   if(sum<3)
   {
      printf("your password is poor \n");
   }
    else if(sum>3) {
      printf("password is good \n");
   }
  printf("--------------------------------------------\n");
   

}

int main()
{
   char pass[20];
   printf("enter the password :");
   scanf("%s",pass);
   printf("%c \n",pass[0]);
  
   check_pass(pass);
}
